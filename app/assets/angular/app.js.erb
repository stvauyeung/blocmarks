blocMarks = angular.module('blocMarks', ['ui.router', 'blocMarksControllers'])

blocMarks.config(['$stateProvider', '$locationProvider', function($stateProvider, $locationProvider) {
  $locationProvider.html5Mode(true);
  $stateProvider.state('landing', {
    url: '/',
    templateUrl: '<%= asset_path("templates/landing.html") %>'
  });
}]);